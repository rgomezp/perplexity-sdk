    }
  },
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build && ./bump"
  },
  "dependencies": {
    "@types/node": "^20.11.16",
    "@types/node-fetch": "^2.5.7",
    "es6-promise": "^4.2.4",
    "form-data": "^2.5.0",
    "node-fetch": "^2.6.0",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "@types/url-parse": "1.4.4",
    "typescript": "^4.0"
  }
}
